{
  "name": "madnss",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "madnss": "./bin/madnss.js"
  },
  "scripts": {
    "dev": "node bin/madnss.js init dev && concurrently -n watch,serve \"nodemon -e js,html --ignore dev --exec \\\"bin/madnss.js -i dev/src -o dev/public -w\\\"\" \"bin/madnss.js serve dev/public\""
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.4.1",
    "browser-sync": "^2.27.7",
    "chokidar": "^3.5.2",
    "commander": "^8.3.0",
    "markdown-it": "^12.2.0",
    "markdown-it-attrs": "^4.1.0",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14"
  }
}
